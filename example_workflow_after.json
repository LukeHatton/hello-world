{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "example.png"
    }
  },
  "2": {
    "class_type": "Florence2ModelLoader",
    "inputs": {
      "model": "microsoft/florence-2-large",
      "precision": "fp16",
      "attention": "sdpa"
    }
  },
  "3": {
    "class_type": "Florence2Run",
    "inputs": {
      "model": ["2", 0],
      "image": ["1", 0],
      "task": "object_detection",
      "text_input": "Detect person, car, dog, and cat in the image",
      "fill_mask": true,
      "max_new_tokens": 1024,
      "num_beams": 3,
      "do_sample": true,
      "output_mask_select": "",
      "seed": 42,
      "keep_model_loaded": true
    }
  },
  "4": {
    "class_type": "SAMModelLoader",
    "inputs": {
      "model_name": "sam_vit_h (2.56GB)",
      "device_mode": "AUTO"
    }
  },
  "5": {
    "class_type": "Florence2toCoordinates",
    "inputs": {
      "florence2_result": ["3", 0],
      "index": "all"
    }
  },
  "6": {
    "class_type": "SAM2",
    "inputs": {
      "sam_model": ["4", 0],
      "coordinates": ["5", 0],
      "image": ["1", 0],
      "individual_objects": true
    }
  },
  "7": {
    "class_type": "PreviewImage",
    "inputs": {
      "images": ["6", 0]
    }
  }
}
